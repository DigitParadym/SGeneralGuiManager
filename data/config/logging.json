{
    "version": 1,
    "log_level": "INFO",
    "max_bytes": 5242880,
    "backup_count": 5,
    "file_logging": true,
    "console_logging": true,
    "rotation_interval": "D",
    "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
    "detailed_format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s - [%(pathname)s:%(lineno)d]",
    "retention_days": 30,
    "error_trace": true
}